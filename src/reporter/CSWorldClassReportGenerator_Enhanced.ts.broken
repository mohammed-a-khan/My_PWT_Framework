// Test result types - matching backup framework structure
interface TestStep {
    name: string;
    status: 'passed' | 'failed' | 'skipped';
    duration?: number;
    error?: string;
    screenshot?: string;
    actions?: Array<{
        action: string;
        status: string;
        duration?: number;
    }>;
    testData?: {
        headers: string[];
        values: any[];
        totalColumns: number;
        usedColumns: string[];
        iterationNumber: number;
        totalIterations: number;
        source?: {
            type: string;
            file?: string;
            sheet?: string;
            delimiter?: string;
            filter?: string;
            connection?: string;
            query?: string;
        };
    };
}

interface TestScenario {
    name: string;
    status: 'passed' | 'failed' | 'skipped';
    duration?: number;
    steps: TestStep[];
    tags?: string[];
    feature?: string;
    testData?: any;
    startTime?: Date;
    endTime?: Date;
}

// Main TestSuite interface - compatible with backup framework
interface TestSuite {
    name: string;
    scenarios: TestScenario[];
    startTime: Date;
    endTime: Date;
    duration?: number;
    totalScenarios?: number;
    passedScenarios?: number;
    failedScenarios?: number;
    skippedScenarios?: number;
}

